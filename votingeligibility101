#include<iostream>
using namespace std;
int main()
{
    string name;
    int age;
     cout<<"VOTING ELIGIBLITY\n";
     cout<<"Name: \n";
     cin>>name;
     cout<<"Age: \n";
     cin>>age;


     if(age>= 18)
     {
         cout<<name<<" you are eligible to vote";
     }

     else
     {
         cout<<name<<" you are not eligible to vote";
         cout<<"NOTE* You must be of age 18 and above to be eligible to vote";
     }
     return 0;
}
